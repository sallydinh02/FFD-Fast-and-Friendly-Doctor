<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/doctorVideoCallContainer"
    android:background="#f2f0f2"
    tools:context=".fragmentDoctor.DoctorVideoCallFragment">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="431dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/healthcareimg5" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="400dp"
        android:orientation="vertical"
        >

        <ImageView
            android:id="@+id/doctorIconVideoCall"
            android:layout_width="109dp"
            android:layout_height="142dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="45dp"
            android:clickable="true"
            android:src="@drawable/callicon3"
            android:elevation="20dp"
            />

    </LinearLayout>

</RelativeLayout>